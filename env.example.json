{
  "app": {
    "PORT": "3000",
    "ORIGIN": ["http://localhost:3080"]
  },
  "db": {
    "DATABASE_URL": "postgres://postgres-url"
  },
  "s3": {
    "BUCKET_NAME": "your-bucket-name",
    "BUCKET_REGION": "your-bucket-region",
    "BUCKET_KEY": "your-bucket-key",

    "ACCESS_COOKIE_NAME": "auth",

    "REFRESH_TOKEN_SECRET": "your-secret",
    "REFRESH_TOKEN_EXPIRES_IN": "30d",
    "REFRESH_COOKIE_NAME": "refresh"
  }
}
